{
  "name": "koa-typescript-starter",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "CC0-1.0",
  "scripts": {
    "build-server": "tslint --project . && tsc",
    "watch-server": "cross-env NODE_ENV=development nodemon --watch 'src/**/*' -e ts,tsx --exec 'ts-node' src/server/index.ts",
    "test": "ava-ts --verbose",
    "start": "node dist/index.js",
    "deploy": "yarn build-server && gcloud app deploy"
  },
  "devDependencies": {
    "@types/immutable": "3.8.7",
    "@types/koa__cors": "2.2.2",
    "@types/koa-router": "7.0.23",
    "@types/koa": "2.0.39",
    "@types/lodash": "4.14.112",
    "ava-ts": "0.24.6",
    "ava": "0.25.0",
    "cross-env": "5.0.5",
    "nodemon": "1.12.0",
    "ts-node": "3.3.0",
    "tslint": "5.7.0",
    "tslint-config-prettier": "1.15.0",
    "typescript": "2.5.2"
  },
  "dependencies": {
    "@koa/cors": "2",
    "axios": "0.18.0",
    "immutable": "3.8.2",
    "koa-router": "7.2.1",
    "koa": "2.3.0",
    "lodash": "4.17.10"
  },
  "engines": {
    "node": "9.x"
  }
}
